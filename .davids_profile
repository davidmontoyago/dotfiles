export JRUBY_OPTS="-J-Xmx1g -J-Xms1g -J-XX:MaxPermSize=512M -Xcompile.invokedynamic=false"

source ~/.aliases

# Set CLICOLOR if you want Ansi Colors in iTerm2 
export CLICOLOR=1

# Set colors to match iTerm2 Terminal Colors
export TERM=xterm

